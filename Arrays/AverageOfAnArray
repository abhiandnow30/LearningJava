public class AcerageOfAnArray {
    public static void main(String[] args) {
        int[] arr={1,2,3,4,5,6};
        int sum=0;
        int k= arr.length;
        for(int i=0;i< arr.length;i++){
            sum=sum+arr[i];
        }
        int average=sum/k;
        System.out.println(average);
    }
}
