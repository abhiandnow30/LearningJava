class ReverseAnSubArray {
    public static void main(String[] args) {
        int[] arr={1,2,3,4,5,6,7,8,9};
        int k=3;
        int temp;
        for(int i=0;i<arr.length;i=i+k){
            int left=arr[i];
            int right=arr[i+2];
            temp=arr[i];
            arr[i]=arr[i+2];
            arr[i+2]=temp;
             }
        for(int i=0;i<arr.length;i++){
        System.out.print(arr[i]+" ");
    }
}
}
