class p20
{
	public static void main(String[] args)
	{
		int c;
		for(int i=0;i<100;i++)
		{
			if(i==0)
			{
				System.out.print('A');
			}
				else
					{
						if(i%2!=0)
							{
								System.out.print(",B");
							}
							else
								{
									System.out.print(",A");
								}
					}
			
		}
	}
}
