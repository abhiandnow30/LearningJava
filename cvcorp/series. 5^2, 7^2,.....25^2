class p23
{
	public static void main(String[] args)
		{
		int count =0;
			for(int i=5;i<25;i++)
				{
					if(i%2!=0)
						{
						if(count==0)
							{
							System.out.print(i+ "^" + "2");
							count=1;
							}
								else
								{
								System.out.print(","+i+ "^" + "2");
								}
						}
								
						
				}
		}
}
