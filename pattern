to print Numbers in below pattern
1
22
333
4444
55555



import java.util.Scanner;
public class Number {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        for(int i=1;i<=a;i++)
//        for pointing rows 
        {
            for(int j=1;j<=i;j++)
//            for pointing columns
            {
                System.out.print(i);
            }
            System.out.println();
//            to move for next row
        }
    }
}
